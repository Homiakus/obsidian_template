{
  "$id": "schemas/part.json",
  "type": "object",
  "required": [
    "id",
    "schema",
    "type",
    "code",
    "title",
    "created"
  ],
  "properties": {
    "schema": {
      "const": "v1"
    },
    "id": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}-\\d{2,6}$"
    },
    "type": {
      "const": "part"
    },
    "code": {
      "type": "string",
      "pattern": "^PART-[A-Z0-9-]+$"
    },
    "title": {
      "type": "string"
    },
    "spec": {
      "type": "string"
    },
    "material": {
      "type": "string"
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "model_units": {
      "enum": [
        "mm",
        "cm",
        "m",
        "in",
        "ft"
      ]
    },
    "model_axis": {
      "enum": [
        "+Zup",
        "+Yup"
      ]
    },
    "model_scale": {
      "type": "number"
    }
  },
  "additionalProperties": true
}